def flips_until_heads(data_list, num_data_points):
    for i in range(num_data_points):
        tails_count = 0
        flip = flip_coin()
        while flip == 'tails':
            flip = flip_coin()
            tails_count += 1
        data_list.append(tails_count)
    return data_list


